	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "",
		"prisma:gen": "npx prisma generate",
		"prisma:push": "npx prisma db push",
		"prisma:seed": "npx prisma db seed",
		"nest:dev": "rimraf server && nest start --watch",
		"react:dev": "cd react && rimraf build && yarn start",
